{
    "presets": [

        ["@babel/preset-env", {
            "debug": true,
            "targets": [
                "last 2 chrome versions",
                "last 2 firefox versions",
                "last 3 safari versions",
                "last 2 ios versions"
                
            ],
            "corejs": 3,
            "useBuiltIns" : "usage"
        }]
        
    ],

    "plugins": [
        "@babel/plugin-proposal-class-properties"
    ]
}